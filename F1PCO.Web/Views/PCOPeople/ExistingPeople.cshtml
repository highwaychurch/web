@model IEnumerable<F1PCO.PCOPerson>

<h2>People we've seen before</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>F1ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>DOB</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var person in Model)
    {
        <tr>
            <td>@person.PCOID</td>
            <td>@person.FirstName</td>
            <td>@person.LastName</td>
            <td>@person.DateOfBirth</td>
        </tr>
    }
    </tbody>
</table>